#! /bin/bash
SESSION="notes"
if ! (tmux has-session -t "$SESSION" >/dev/null 2>&1); then
  TMUX_PREV=$TMUX
  TMUX=

  tmux new-session -s "$SESSION" -n notes -c "$HOME/Dropbox/Information/" -d
  tmux send-keys -t "$SESSION" "$EDITOR ." C-m

  TMUX=$TMUX_PREV

  tat "$SESSION"
else
  tat "$SESSION"
fi
